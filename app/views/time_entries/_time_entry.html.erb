<div id="<%= dom_id time_entry %>">
  <p>
    <strong>Start time:</strong>
    <%= time_entry.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= time_entry.end_time %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= time_entry.description %>
  </p>

  <p>
    <strong>Task:</strong>
    <%= time_entry.task %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= time_entry.notes %>
  </p>

</div>
