<%= line_chart @time_entries %>


<%= form_with model: @time_entry do |f| %>
  <%= f.label :task_id %>
  <%= f.collection_select :task_id, @tasks, :id, :name, prompt: "Select a task" %>

  <%= f.label :start_time %>
  <%= f.datetime_local_field :start_time %>

  <%= f.label :end_time %>
  <%= f.datetime_local_field :end_time %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit %>
<% end %>

